{
    "api": {
        "version": "1.0.0",
        "host": "localhost",
        "port": 5000,
        "debug": true,
        "allowed_origins": [
            "http://localhost:8080",
            "http://localhost:3000",
            "http://localhost:5000",
            "http://localhost:8000",
            "http://localhost",
            "http://127.0.0.1:8080",
            "http://127.0.0.1:3000",
            "http://127.0.0.1:5000",
            "http://127.0.0.1:8000",
            "http://127.0.0.1",
            "null"
        ],
        "cors_settings": {
            "allow_credentials": true,
            "allow_methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD"],
            "allow_headers": [
                "Content-Type",
                "Accept",
                "Authorization",
                "X-Requested-With",
                "X-API-Key"
            ],
            "max_age": 3600
        }
    },
    "agents": {
        "max_agents": 10,
        "default_capabilities": ["text_processing", "web_search", "code_analysis"],
        "roles": {
            "researcher": {
                "goal": "Conduct thorough research and provide accurate information",
                "capabilities": ["web_search", "text_processing"]
            },
            "coder": {
                "goal": "Write, analyze, and debug code efficiently",
                "capabilities": ["code_analysis", "text_processing"]
            },
            "assistant": {
                "goal": "Provide general assistance and coordinate with other agents",
                "capabilities": ["text_processing", "web_search"]
            }
        }
    },
    "tools": {
        "web_search": {
            "endpoint": "/api/tools/web_search",
            "rate_limit": 60,
            "timeout": 30
        },
        "code_analysis": {
            "endpoint": "/api/tools/code_analysis",
            "rate_limit": 60,
            "timeout": 45
        },
        "text_processing": {
            "endpoint": "/api/tools/text_processing",
            "rate_limit": 120,
            "timeout": 30
        }
    },
    "system": {
        "max_context_size": 4096,
        "max_message_length": 2000,
        "max_conversation_history": 100,
        "metrics": {
            "update_interval": 30,
            "history_points": 12
        },
        "websocket": {
            "ping_interval": 30,
            "timeout": 60
        },
        "safety": {
            "max_tokens_per_request": 2048,
            "max_concurrent_requests": 5,
            "rate_limit_window": 60
        }
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "handlers": {
            "console": {
                "enabled": true,
                "level": "INFO"
            },
            "file": {
                "enabled": true,
                "level": "DEBUG",
                "filename": "orchestrator.log",
                "max_size": 10485760,
                "backup_count": 5
            }
        }
    }
} 